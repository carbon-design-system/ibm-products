{
  "name": "@carbon/ibm-products-web-components",
  "description": "Carbon for IBM Products Web Components",
  "version": "0.31.0",
  "license": "Apache-2.0",
  "main": "es/index.js",
  "module": "es/index.js",
  "type": "module",
  "bugs": "https://github.com/carbon-design-system/ibm-products/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/ibm-products.git",
    "directory": "packages/ibm-products-web-components"
  },
  "files": [
    "custom-elements.json",
    "es/**/*",
    "es-custom/**/*",
    "lib/**/*",
    "scss/**/*",
    "telemetry.yml"
  ],
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "exports": {
    "./es/*": "./es/*",
    "./es-custom/*": "./es-custom/*",
    "./lib/*": "./lib/*",
    "./dist/*": "./dist/*",
    "./scss/*": "./scss/*",
    "./custom-elements.json": "./custom-elements.json",
    "./package.json": "./package.json",
    "./telemetry.yml": "./telemetry.yml"
  },
  "keywords": [
    "carbon",
    "carbon design system",
    "carbon community",
    "carbon for ibm products",
    "carbon for ibm products web components",
    "web components"
  ],
  "scripts": {
    "build": "yarn clean && node tasks/build.js && yarn wca",
    "build:storybook": "storybook build --quiet",
    "clean": "rimraf es lib scss dist storybook-static",
    "postinstall": "ibmtelemetry --config=telemetry.yml",
    "preview": "vite preview",
    "storybook": "storybook dev -p 3000",
    "test": "yarn playwright install --with-deps && vitest run",
    "wca": "web-component-analyzer analyze src --outFile custom-elements.json",
    "coverage": "vitest run --coverage",
    "telemetry-config": "ibmtelemetry-config generate --wc --id 8a79d901-a9c0-4672-94a4-fa8eea139832 --endpoint https://www-api.ibm.com/ibm-telemetry/v1/metrics --files ./src/components"
  },
  "dependencies": {
    "@carbon-labs/wc-empty-state": "^0.11.0",
    "@carbon/ibm-products-styles": "^2.78.0",
    "@carbon/styles": "^1.96.0",
    "@carbon/utilities": "^0.15.0",
    "@carbon/web-components": "2.44.0",
    "@figma/code-connect": "^1.3.6",
    "@ibm/telemetry-js": "^1.10.2",
    "@lit-labs/signals": "^0.2.0",
    "@lit/context": "^1.1.5",
    "lit": "^3.1.0"
  },
  "devDependencies": {
    "@aarsteinmedia/dotlottie-player": "^6.0.5",
    "@blockquote/rollup-plugin-externalize-source-dependencies": "^1.0.0",
    "@carbon/icon-helpers": "^10.69.0",
    "@carbon/icons": "^11.71.0",
    "@carbon/motion": "^11.38.0",
    "@figma/code-connect": "^1.3.6",
    "@ibm/telemetry-js-config-generator": "^2.1.0",
    "@mordech/vite-lit-loader": "^0.37.0",
    "@open-wc/testing": "^4.0.0",
    "@rollup/plugin-alias": "^6.0.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@stackblitz/sdk": "^1.11.0",
    "@storybook/addon-a11y": "^9.1.10",
    "@storybook/addon-docs": "^9.1.10",
    "@storybook/addon-links": "^9.1.10",
    "@storybook/web-components-vite": "^9.1.10",
    "@types/jest": "^29.5.13",
    "@vitest/browser": "^3.2.0",
    "@vitest/coverage-v8": "^3.2.0",
    "autoprefixer": "^10.4.20",
    "c8": "^10.1.3",
    "cssnano": "^7.0.6",
    "eslint": "^9.14.0",
    "eslint-config-carbon": "^3.25.0",
    "globby": "^15.0.0",
    "happy-dom": "^20.0.0",
    "playwright": "^1.56.1",
    "postcss": "^8.4.47",
    "remark-gfm": "^4.0.0",
    "rimraf": "^6.0.1",
    "rollup": "^4.27.3",
    "rollup-plugin-copy": "^3.5.0",
    "sass": "^1.93.2",
    "storybook": "^9.1.10",
    "storybook-addon-accessibility-checker": ">=9.2.0-rc.0",
    "tslib": "^2.8.1",
    "typescript": "^5.5.3",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.0",
    "web-component-analyzer": "2.0.0"
  }
}
