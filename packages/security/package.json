{
  "name": "@carbon/ibm-cloud-cognitive-security",
  "description": "Carbon for Cloud & Cognitive IBM Security UI components",
  "version": "0.9.0",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/ibm-cloud-cognitive.git",
    "directory": "packages/security"
  },
  "bugs": "https://github.com/carbon-design-system/ibm-cloud-cognitive/issues",
  "files": [
    "css",
    "es",
    "lib",
    "scss"
  ],
  "keywords": [
    "carbon",
    "carbon design system",
    "carbon community",
    "carbon for cloud & cognitive",
    "ibm security"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "run-s clean build-all",
    "build-all": "run-p build:*",
    "build:css": "bundler bundle:scss src/index.scss",
    "build:js-esm": "cross-env BABEL_ENV=esm yarn build:js:modules -d es",
    "build:js-cjs": "cross-env BABEL_ENV=cjs yarn build:js:modules -d lib",
    "build:js:modules": "babel src --ignore '**/_mocks_','**/__tests__','**/*+(-story|.stories).*'",
    "build:scss": "copyfiles 'src/**/*.scss' scss -u 1",
    "ci-check": "node scripts/import",
    "clean": "rimraf css es lib scss",
    "clean:ibm-security": "rimraf ibm-security",
    "clean:src": "rimraf src",
    "sync": "run-s clean:src sync:* clean:ibm-security",
    "sync:clone": "git clone git@github.com:carbon-design-system/ibm-security.git --depth 1",
    "sync:copy": "copyfiles 'ibm-security/src/**/*' . -u 1",
    "test": "run-p test:*",
    "test:js": "jest",
    "test:scss": "bundler check 'src/**/*.scss' -i '**/css-gridish/**'",
    "upgrade-dependencies": "npm-check-updates -u --color --reject '/(carbon)/'"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@carbon/colors": "10.29.0",
    "@carbon/grid": "10.32.0",
    "@carbon/icons-react": "10.36.0",
    "@carbon/import-once": "10.5.0",
    "@carbon/layout": "10.28.0",
    "@carbon/motion": "10.21.0",
    "@carbon/themes": "10.39.0",
    "@carbon/type": "10.32.0",
    "carbon-components": "10.40.1",
    "carbon-components-react": "7.40.1",
    "carbon-icons": "7.0.7",
    "classnames": "^2.3.1",
    "downshift": "^6.1.5",
    "export-to-csv": "^0.2.1",
    "focus-trap-react": "^8.7.0",
    "invariant": "^2.2.4",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react-transition-group": "^4.4.2",
    "throttle-debounce": "^3.0.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-export-default-from": "^7.14.5",
    "@carbon/bundler": "^10.11.0",
    "babel-plugin-macros": "^3.1.0",
    "babel-preset-ibm-cloud-cognitive": "^0.7.0",
    "data-uri.macro": "^1.0.0",
    "deepmerge": "^4.2.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint-plugin-only-warn": "^1.0.3",
    "jest-config-ibm-cloud-cognitive": "^0.8.0",
    "node-sass": "^6.0.0",
    "recompose": "^0.30.0"
  }
}
